{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "yarn"
  },
  "generators": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": false
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "avidtrader",
  "projects": {
    "agx-assets": "libs/agx-assets",
    "agx-dto": "libs/agx-dto",
    "agx-store": "libs/agx-store",
    "agx-util": "libs/agx-util",
    "api": "apps/api",
    "avidtrader": "apps/avidtrader",
    "avidtrader-e2e": "apps/avidtrader-e2e",
    "nax-ipware": "libs/nax-ipware",
    "ngx-auth": "libs/ngx-auth",
    "ngx-cachify": "libs/ngx-cachify",
    "ngx-config": "libs/ngx-config",
    "ngx-gql": "libs/ngx-gql",
    "ngx-gtag": "libs/ngx-gtag",
    "ngx-i18n": "libs/ngx-i18n",
    "ngx-jwt": "libs/ngx-jwt",
    "ngx-layout": "libs/ngx-layout",
    "ngx-logger": "libs/ngx-logger",
    "ngx-material": "libs/ngx-material",
    "ngx-menu": "libs/ngx-menu",
    "ngx-msg": "libs/ngx-msg",
    "ngx-shared": "libs/ngx-shared",
    "ngx-store": "libs/ngx-store",
    "ngx-subify": "libs/ngx-subify",
    "ngx-system": "libs/ngx-system",
    "ngx-uix": "libs/ngx-uix",
    "ngx-user": "libs/ngx-user",
    "nsx-auth": "libs/nsx-auth",
    "nsx-common": "libs/nsx-common",
    "nsx-mailer": "libs/nsx-mailer",
    "nsx-prisma": "libs/nsx-prisma",
    "nsx-system": "libs/nsx-system",
    "nsx-user": "libs/nsx-user",
    "nsx-i18n": "libs/nsx-i18n"
  }
}
