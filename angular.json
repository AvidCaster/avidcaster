{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "yarn"
  },
  "defaultProject": "fullerstack",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": false
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "fullerstack": {
      "projectType": "application",
      "root": "apps/fullerstack",
      "sourceRoot": "apps/fullerstack/src",
      "prefix": "fullerstack",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": ["zen-observable"],
            "outputPath": "dist/apps/fullerstack",
            "index": "apps/fullerstack/src/index.html",
            "main": "apps/fullerstack/src/main.ts",
            "polyfills": "apps/fullerstack/src/polyfills.ts",
            "tsConfig": "apps/fullerstack/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/fullerstack/src/favicon.ico",
              "apps/fullerstack/src/assets",
              {
                "glob": "**/*",
                "input": "libs/agx-assets/src/lib/images",
                "output": "assets/images"
              }
            ],
            "styles": ["apps/fullerstack/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/fullerstack/src/environments/environment.ts",
                  "with": "apps/fullerstack/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "ci": {
              "fileReplacements": [
                {
                  "replace": "apps/fullerstack/src/environments/environment.ts",
                  "with": "apps/fullerstack/src/environments/environment.ci.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "fullerstack:build",
            "proxyConfig": "apps/fullerstack/proxy/proxy.dev.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "fullerstack:build:production",
              "proxyConfig": "apps/fullerstack/proxy/proxy.prod.json",
              "port": 4300
            },
            "ci": {
              "browserTarget": "fullerstack:build:ci",
              "proxyConfig": "apps/fullerstack/proxy/proxy.ci.json",
              "port": 4400
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/fullerstack/src/**/*.ts",
              "apps/fullerstack/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/fullerstack"],
          "options": {
            "jestConfig": "apps/fullerstack/jest.config.js",
            "passWithNoTests": true
          },
          "ci": {
            "sourceMap": false,
            "codeCoverage": true,
            "progress": false,
            "watch": false
          }
        }
      }
    },
    "fullerstack-e2e": {
      "root": "apps/fullerstack-e2e",
      "sourceRoot": "apps/fullerstack-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/fullerstack-e2e/cypress.json",
            "tsConfig": "apps/fullerstack-e2e/tsconfig.e2e.json",
            "devServerTarget": "fullerstack:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fullerstack:serve:production",
              "port": 4300
            },
            "ci": {
              "devServerTarget": "fullerstack:serve:ci",
              "port": 4400
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/fullerstack-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            },
            "ci": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.ci.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build",
            "inspect": true,
            "port": 9229
          },
          "configurations": {
            "production": {
              "buildTarget": "api:build:production"
            },
            "ci": {
              "buildTarget": "api:build:ci"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        },
        "ci": {
          "sourceMap": false,
          "codeCoverage": true,
          "progress": false,
          "watch": false
        }
      }
    },
    "api-dto": {
