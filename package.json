    "js-base64": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash-es": "^4.17.21",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.3",
