    "@nrwl/workspace": "12.3.6",
    "@types/cookie-parser": "^1.4.2",
    "@types/crypto-js": "^4.0.1",
    "@types/gtag.js": "^0.0.4",
    "@types/jest": "26.0.8",
    "@types/lodash": "^4.14.168",
